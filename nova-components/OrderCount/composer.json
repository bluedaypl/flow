{
    "name": "blueday/order-count",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Blueday\\OrderCount\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blueday\\OrderCount\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
